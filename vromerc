set disablesites=mail.google.com,docs.google.com
set enable_vrome_key=<C-z>
" set autocomplete_prev=<C-n>
" set autocomplete_next=<C-p>

unmap f  " hint
set hintkeys=jlkhfsdagwerui
" set useletters=1

map x <M-d>
unmap <M-f>
unmap <M-l>

map j 5j
map k 5k
map d <C-d>
map u <C-u>

" traverse tabs
map l gt
map h gT

" Change external editor, default is GVim
set editor=gvim -f

" Remove key mapping in normal mode, bring back normal search
unmap <C-f>
unmap <M-d>

########################################################################  rbenv



 case $(uname -s) in
     Darwin)
 [ -d $HOME/.rbenv/ ]  && export PATH="$HOME/.rbenv/bin:$PATH"
 [ -d $HOME/.rbenv/ ]  && export PATH="$HOME/.rbenv/shims:$PATH"
 [ -d $HOME/.rbenv/ ]  && eval "$(rbenv init - zsh)"
     ;;
 esac

